{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureDataFactoryVinny001"
		},
		"LS_ADLSConnectionVinny_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSConnectionVinny'"
		},
		"LS_BlobConnectionVinny_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobConnectionVinny'"
		},
		"LS_SQLconnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLconnection'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=azuresqlservervinny.database.windows.net;Initial Catalog=azuresqldatabasevinny;User ID=Vinatha"
		},
		"LS_ADLSConnectionVinny_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfladlsgen2vinny.dfs.core.windows.net/"
		},
		"LS_ADLSConnectionVinny2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfladlsgen2vinny.dfs.core.windows.net/"
		},
		"LS_BlobConnectionVinny2_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "BlobConnectionString"
		},
		"LS_HTTPConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"LS_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adfkeyvalutvinny.vault.azure.net/"
		},
		"Storage trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d554af37-9aeb-4bab-83a7-a144f368257f/resourceGroups/ADFVinny/providers/Microsoft.Storage/storageAccounts/adfblobstoragevinny"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzureDataFactoryVinny001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSConnectionVinny_accountKey": {
						"value": "[parameters('LS_ADLSConnectionVinny_accountKey')]"
					},
					"LS_BlobConnectionVinny_connectionString": {
						"value": "[parameters('LS_BlobConnectionVinny_connectionString')]"
					},
					"LS_SQLconnection_connectionString": {
						"value": "[parameters('LS_SQLconnection_connectionString')]"
					},
					"LS_ADLSConnectionVinny_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSConnectionVinny_properties_typeProperties_url')]"
					},
					"LS_HTTPConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTPConnection_properties_typeProperties_url')]"
					},
					"LS_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLSConnectionVinny2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSConnectionVinny2_properties_typeProperties_url')]"
					},
					"LS_BlobConnectionVinny2_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_BlobConnectionVinny2_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "AzureDataFactoryVinny001_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AzureDataFactoryVinny001_ArmTemplate_0')]"
			]
		},
		{
			"name": "AzureDataFactoryVinny001_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Storage trigger_properties_typeProperties_scope": {
						"value": "[parameters('Storage trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AzureDataFactoryVinny001_ArmTemplate_1')]"
			]
		}
	]
}